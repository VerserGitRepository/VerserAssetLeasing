@model IEnumerable<VerserAssetleasingServiceInterface.Models.AssetsListViewModel>
<script src="~/Scripts/jquery-3.4.1.min.js" type="text/javascript"></script>
<link href="~/Content/bootstrap.css" rel="stylesheet" />
<script src="~/Scripts/jquery-3.4.1.min.js" type="text/javascript"></script>
<link href="https://cdn.datatables.net/1.10.19/css/jquery.dataTables.min.css" rel="stylesheet" />
<script src="https://code.jquery.com/jquery-3.3.1.js"></script>
<script src=" https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js"></script>
<script src="https://cdn.datatables.net/1.10.15/js/dataTables.bootstrap4.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
<h4 align="center" style="color:GrayText;font-weight:600;"></h4>
<br>

<div class="container" style="margin-left:12px;padding-top:2px">
    <div class="row" style="width:120%">
        <div class="col-md-12">
            <table class="table table-bordered  table-responsive table-hover" id="mainTable" @*style="font-size:14px;width:132%;margin-right:10px;color:black"*@>
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.AssetID)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.SerialNo)
                        </th>

                        <th>
                            @Html.DisplayNameFor(model => model.IMEI)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.SIMNo)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.PurchaseOrderNo)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.PurchaseDate)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.PurchasePrice)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.DecomissionDate)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Condition)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.BuyBackPrice)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.AquisitionDate)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.SPMD)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.FirstCommissionDate)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.LastCommissionDate)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.LastRepairDate)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.CreatedBy)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Created)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.ModifiedBy)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Modified)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Asset_Contract)
                        </th>

                        <th>
                            @Html.DisplayNameFor(model => model.Asset_LifecycleStatus)
                        </th>

                        <th>
                            @Html.DisplayNameFor(model => model.Asset_OSVersion)
                        </th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.AssetID)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.SerialNo)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Asset_Company)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.IMEI)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.SIMNo)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.PurchaseOrderNo)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.PurchaseDate)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.PurchasePrice)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.DecomissionDate)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Condition)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.BuyBackPrice)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.AquisitionDate)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.SPMD)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.FirstCommissionDate)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.LastCommissionDate)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.LastRepairDate)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.CreatedBy)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Created)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ModifiedBy)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Modified)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Asset_Contract)
                            </td>

                            <td>
                                @Html.DisplayFor(modelItem => item.Asset_LifecycleStatus)
                            </td>

                            <td>
                                @Html.DisplayFor(modelItem => item.Asset_OSVersion)
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script src="@Url.Content("~/Scripts/jquery-ui-1.12.1.min.js")"></script>
    <script src="~/Scripts/datatables.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $('#mainTable').DataTable();
        });
                    //  ExportTimesSheetToExcel", "Candidates"

    </script>
    <body>
        <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
        <script type="text/javascript" charset="utf8" src="//cdn.datatables.net/1.10.15/js/jquery.dataTables.js"></script>
        <script src="https://cdn.datatables.net/plug-ins/1.10.15/api/row().show().js"></script>
    </body>
}
